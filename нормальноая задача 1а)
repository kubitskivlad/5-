#include <iostream>
using namespace std;
bool CorrectlyPlacedBrackets(char a[])
 {
	char b[100]; int start = 0; bool solution = true;
	for (int i = 0; i < strlen(a); i++)
		 {
	        if ((a[i] == '[') || (a[i] == '{') || (a[i] == '('))
			 {
			b[start] = a[i];
			start++;
			}
		if ((a[i] == ')') && (b[start - 1] == '('))
			+ start--;
	    if ((a[i] == '}') && (b[start - 1] == '{'))
			+ start--;
		if ((a[i] == ']') && (b[start - 1] == '['))
			 start--;
		}
	if (start == 0) solution = true;
	else  solution = false;
	return solution;
	}
int main()
 {
	
	return 0;
	}
